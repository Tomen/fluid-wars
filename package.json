{
  "name": "fluid-wars",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --import tsx scripts/sync-model.ts && vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "train:build": "tsc --project tsconfig.training.json",
    "train": "node --import tsx training/train.ts",
    "train:check": "tsc --project tsconfig.training.json --noEmit",
    "sync-model": "node --import tsx scripts/sync-model.ts"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "esbuild": "^0.27.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "js-yaml": "^4.1.1"
  }
}
